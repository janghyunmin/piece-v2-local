<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="activity"
            type="com.bsstandard.piece.view.deed.DeedActivity" />

        <variable
            name="portfolioDetailViewModel"
            type="com.bsstandard.piece.view.portfolioDetail.PortfolioDetailViewModel" />

        <!-- 구매 정보 조회 ( 소유 조각 ) ViewModel -->
        <variable
            name="purchaseVm"
            type="com.bsstandard.piece.data.viewmodel.PurchaseViewModel" />

        <variable
            name="memberVm"
            type="com.bsstandard.piece.data.viewmodel.GetUserViewModel" />

        <variable
            name="pos"
            type="Integer" />

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:id="@+id/parent_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true"
            android:layout_weight="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/top_layout"
                    android:layout_width="match_parent"
                    android:layout_height="80dp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/back_img"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="12dp"
                        android:background="@drawable/arrow_left"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/top_title"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:lineHeight="25dp"
                        android:text="소유 증서"
                        android:textColor="@color/c_4a4d55"
                        android:textSize="16dp"
                        app:fontFamily="@font/pretendard_regular"
                        app:layout_constraintBottom_toBottomOf="@+id/back_img"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/back_img" />
                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/top_title_layout"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="16dp"
                    android:paddingBottom="100dp"
                    app:layout_constraintTop_toBottomOf="@+id/top_layout">

                    <TextView
                        android:id="@+id/deed_title_1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/pretendard_extrabold"
                        android:gravity="left"
                        android:lineHeight="28dp"
                        android:text="@string/deed_title_1"
                        android:textColor="@color/c_131313"
                        android:textSize="20dp"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/deed_title_2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/layout_round_f2f3f4"
                        android:padding="15dp"
                        app:layout_constraintTop_toBottomOf="@+id/deed_title_1">

                        <TextView
                            android:id="@+id/title_2"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="@string/deed_title_2"
                            android:textSize="16dp" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <!-- 제 1조 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/deed_title_3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/layout_round_f2f3f4"
                        android:padding="15dp"
                        app:layout_constraintTop_toBottomOf="@+id/deed_title_2">

                        <TextView
                            android:id="@+id/parent_title_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/pretendard_bold"
                            android:lineHeight="25dp"
                            android:text="제 1조"
                            android:textColor="@color/c_8c919f"
                            android:textSize="16dp" />

                        <TextView
                            android:id="@+id/title_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="@string/deed_title_3"
                            android:textSize="16dp"
                            app:layout_constraintTop_toBottomOf="@+id/parent_title_1" />
                    </androidx.constraintlayout.widget.ConstraintLayout>


                    <!-- 제 2조 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/deed_title_4"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/layout_round_f2f3f4"
                        android:padding="15dp"
                        app:layout_constraintTop_toBottomOf="@+id/deed_title_3">

                        <TextView
                            android:id="@+id/parent_title_2"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/pretendard_bold"
                            android:lineHeight="25dp"
                            android:text="제 2조 (당사자)"
                            android:textColor="@color/c_8c919f"
                            android:textSize="16dp" />

                        <TextView
                            android:id="@+id/title_4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="@string/deed_title_4"
                            android:textSize="16dp"
                            app:layout_constraintTop_toBottomOf="@+id/parent_title_2" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <!-- 제 3조 ( 진행해야함 )-->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/deed_title_5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/layout_round_f2f3f4"
                        android:padding="15dp"
                        app:layout_constraintTop_toBottomOf="@+id/deed_title_4">

                        <TextView
                            android:id="@+id/parent_title_3"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/pretendard_bold"
                            android:lineHeight="25dp"
                            android:text="제3조 (구매 대상 및 대금 지급)"
                            android:textColor="@color/c_8c919f"
                            android:textSize="16dp" />

                        <TextView
                            android:id="@+id/title_5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="@string/deed_title_5"
                            android:textSize="16dp"
                            app:layout_constraintTop_toBottomOf="@+id/parent_title_3" />

                        <View
                            android:id="@+id/line"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="10dp"
                            android:background="@color/c_8c919f"
                            app:layout_constraintTop_toBottomOf="@+id/title_5" />


                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/p_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="구매 대상"
                            android:textSize="16dp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/line" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/title"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text=""
                            android:textSize="16dp"
                            app:layout_constraintLeft_toRightOf="@+id/p_title"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/line" />

                        <View
                            android:id="@+id/line1"
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginTop="5dp"
                            android:background="@color/c_8c919f"
                            app:layout_constraintTop_toBottomOf="@+id/title" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/p_amount"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="5dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="구매 수량"
                            android:textSize="16dp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/line1" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/amount"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="5dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text=""
                            android:textSize="16dp"
                            app:layout_constraintLeft_toRightOf="@+id/p_amount"
                            app:layout_constraintTop_toBottomOf="@+id/line1" />

                        <View
                            android:id="@+id/line2"
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginTop="5dp"
                            android:background="@color/c_8c919f"
                            app:layout_constraintTop_toBottomOf="@+id/amount" />


                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/p_account"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="5dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="구매 대금"
                            android:textSize="16dp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/line2" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/account"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="5dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text=""
                            android:textSize="16dp"
                            app:layout_constraintLeft_toRightOf="@+id/p_account"
                            app:layout_constraintTop_toBottomOf="@+id/line2" />

                        <View
                            android:id="@+id/line3"
                            android:layout_width="match_parent"
                            android:layout_height="0.5dp"
                            android:layout_marginTop="5dp"
                            android:background="@color/c_8c919f"
                            app:layout_constraintTop_toBottomOf="@+id/account" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/p_confirm"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="5dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="구매 확인"
                            android:textSize="16dp"
                            app:layout_constraintLeft_toLeftOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/line3" />

                        <androidx.appcompat.widget.AppCompatTextView
                            android:id="@+id/confirm"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="5dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text=""
                            android:textSize="16dp"
                            app:layout_constraintLeft_toRightOf="@+id/p_confirm"
                            app:layout_constraintRight_toRightOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/line3" />

                        <View
                            android:id="@+id/line4"
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:layout_marginTop="10dp"
                            android:background="@color/c_8c919f"
                            app:layout_constraintTop_toBottomOf="@+id/confirm" />


                        <TextView
                            android:id="@+id/title_5_1"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="@string/deed_title_5_1"
                            android:textSize="16dp"
                            app:layout_constraintTop_toBottomOf="@+id/line4" />


                    </androidx.constraintlayout.widget.ConstraintLayout>


                    <!-- 제 4조 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/deed_title_6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/layout_round_f2f3f4"
                        android:padding="15dp"
                        app:layout_constraintTop_toBottomOf="@+id/deed_title_5">

                        <TextView
                            android:id="@+id/parent_title_4"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/pretendard_bold"
                            android:lineHeight="25dp"
                            android:text="제4조 (분할 소유권 취득 및 자산 관리 처분 위임의 개시)"
                            android:textColor="@color/c_8c919f"
                            android:textSize="16dp" />

                        <TextView
                            android:id="@+id/title_6"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="@string/deed_title_6"
                            android:textSize="16dp"
                            app:layout_constraintTop_toBottomOf="@+id/parent_title_4" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <!-- 제 5조 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/deed_title_7"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/layout_round_f2f3f4"
                        android:padding="15dp"
                        app:layout_constraintTop_toBottomOf="@+id/deed_title_6">

                        <TextView
                            android:id="@+id/parent_title_5"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/pretendard_bold"
                            android:lineHeight="25dp"
                            android:text="제5조 (분할 소유권의 처분)"
                            android:textColor="@color/c_8c919f"
                            android:textSize="16dp" />

                        <TextView
                            android:id="@+id/title_7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="@string/deed_title_7"
                            android:textSize="16dp"
                            app:layout_constraintTop_toBottomOf="@+id/parent_title_5" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <!-- 제 6조 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/deed_title_8"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/layout_round_f2f3f4"
                        android:padding="15dp"
                        app:layout_constraintTop_toBottomOf="@+id/deed_title_7">

                        <TextView
                            android:id="@+id/parent_title_6"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/pretendard_bold"
                            android:lineHeight="25dp"
                            android:text="제6조 (계약의 목적)"
                            android:textColor="@color/c_8c919f"
                            android:textSize="16dp" />

                        <TextView
                            android:id="@+id/title_8"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="@string/deed_title_8"
                            android:textSize="16dp"
                            app:layout_constraintTop_toBottomOf="@+id/parent_title_6" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <!-- 제 7조 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/deed_title_9"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/layout_round_f2f3f4"
                        android:padding="15dp"
                        app:layout_constraintTop_toBottomOf="@+id/deed_title_8">

                        <TextView
                            android:id="@+id/parent_title_7"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/pretendard_bold"
                            android:lineHeight="25dp"
                            android:text="제7조 (자산에 대한 처분 권한 위임 – 분할소유관계의 해소)"
                            android:textColor="@color/c_8c919f"
                            android:textSize="16dp" />

                        <TextView
                            android:id="@+id/title_9"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="@string/deed_title_9"
                            android:textSize="16dp"
                            app:layout_constraintTop_toBottomOf="@+id/parent_title_7" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <!-- 제 8조 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/deed_title_10"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/layout_round_f2f3f4"
                        android:padding="15dp"
                        app:layout_constraintTop_toBottomOf="@+id/deed_title_9">

                        <TextView
                            android:id="@+id/parent_title_8"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/pretendard_bold"
                            android:lineHeight="25dp"
                            android:text="제8조 (매각 대금 정산 및 수수료)"
                            android:textColor="@color/c_8c919f"
                            android:textSize="16dp" />

                        <TextView
                            android:id="@+id/title_10"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="@string/deed_title_10"
                            android:textSize="16dp"
                            app:layout_constraintTop_toBottomOf="@+id/parent_title_8" />

                    </androidx.constraintlayout.widget.ConstraintLayout>


                    <!-- 제 9조 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/deed_title_11"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/layout_round_f2f3f4"
                        android:padding="15dp"
                        app:layout_constraintTop_toBottomOf="@+id/deed_title_10">

                        <TextView
                            android:id="@+id/parent_title_9"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/pretendard_bold"
                            android:lineHeight="25dp"
                            android:text="제9조 (예치금 관리)"
                            android:textColor="@color/c_8c919f"
                            android:textSize="16dp" />

                        <TextView
                            android:id="@+id/title_11"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:fontFamily="@font/pretendard_regular"
                            android:lineHeight="25dp"
                            android:text="@string/deed_title_11"
                            android:textSize="16dp"
                            app:layout_constraintTop_toBottomOf="@+id/parent_title_9" />

                    </androidx.constraintlayout.widget.ConstraintLayout>

                    <!-- 제 10조 -->
                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/deed_title_12"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:background="@drawable/layout_round_f2f3f4"
