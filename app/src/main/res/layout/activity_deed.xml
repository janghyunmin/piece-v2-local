                        app:layout_constraintTop_toBottomOf="@+id/deed_title_12">

                        <TextView
                            android:id="@+id/parent_title_11"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/pretendard_bold"
